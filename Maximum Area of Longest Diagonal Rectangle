class Solution:
    def areaOfMaxDiagonal(self, dimensions):
        max_diag = -1
        max_area = 0
        
        for l, w in dimensions:
            diag_sq = l*l + w*w   # compare diagonal^2
            area = l*w
            if diag_sq > max_diag:
                max_diag = diag_sq
                max_area = area
            elif diag_sq == max_diag and area > max_area:
                max_area = area
        
        return max_area


